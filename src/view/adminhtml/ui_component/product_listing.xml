<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="create_easytranslate_project">
                <settings>
                    <type>createEasytranslateProject</type>
                    <label translate="true">Create Easytranslate Project</label>
                    <url path="easytranslate/product/createProjectFromProduct"/>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
